@article{kaushik_design_nodate,
	title = {{DESIGN} {OF} {LOW} {POWER} {CMOS} {LOW} {PASS} {FILTER} {FOR} {BIOMEDICAL} {APPLICATION}},
	abstract = {The demand for biomedical monitoring products is increasing worldwide. Low power is the most important concern/factor that need in circuits used for personal health monitoring application to achieve a long battery life. This Paper present the design method of low frequency CMOS low pass filter for biomedical application. In this design technique OTA is used as the basic building block of low pass filter. In this proposed design the multi-tanh doublet technique is used. In this proposed design second order low pass filter is design using the CMOS 90nm technology with ±0.35V supply voltage. The frequency response of low pass filter for EEC and EEG biomedical application is 250Hz and 200Hz respectably. The power consumption of the circuit is 13.43nW for ECG and 9.4nW for EEG respectably. The circuit is tuneable using the control voltage.},
	language = {en},
	author = {Kaushik, Puneet and Jain, Mohit},
